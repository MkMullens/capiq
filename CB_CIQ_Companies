   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "capiq_companies['company_name_mod'] = capiq_companies['company_name']\n",
    "\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace(r\"\\(.*\\)\",\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( ', Inc\\.',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( ', LLC',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Inc\\.',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Corp\\.',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Corporation',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Group Holdings',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Company',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'Co\\.',\"\")\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].str.replace( 'PLC',\"\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].apply(lambda x: x.strip())\n",
    "\n",
    "capiq_companies['company_name_mod'] = capiq_companies['company_name_mod'].apply(lambda x: x.upper())"
   ]
