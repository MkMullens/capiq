   "cell_type": "code",
   "execution_count": 14,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name']\n",
    "\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( ', Inc\\.',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( ', LLC',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Inc\\.',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Corp\\.',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Corporation',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Group Holdings',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Company',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'Co\\.',\"\")\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].str.replace( 'PLC',\"\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].apply(lambda x: x.strip())\n",
    "\n",
    "Funded_Companies['company_name_mod'] = Funded_Companies['company_name_mod'].apply(lambda x: x.upper())"
   ]
  },
